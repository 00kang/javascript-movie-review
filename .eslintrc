{
  "rules": {
    "max-depth": ["error", 1],
    "max-params": ["error", 3],
    "max-lines-per-function": ["error", 10],
    "import/extensions": "off",
  },
  "env": {
    "es6": true,
    "node": true,
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "airbnb-base",
  ],
}
